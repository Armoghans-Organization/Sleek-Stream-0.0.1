{
  "name": "sleek-stream",
  "version": "1.0.0",
  "description": "Custom Hugo theme with Tailwind CSS, SCSS, Summery Panel, Admin Panel, and Light/Dark theme support.",
  "scripts": {
    "npm-check": "npm-check -u",
    "fullinstall": "npm run preinstall && npm install && npm run postinstall",
    "preinstall": "rimraf assets/vendor",
    "postinstall": "vendor-copy",
    "assets": "rimraf assets/vendor && vendor-copy",
    "start": "hugo server --buildDrafts --buildFuture --buildExpired --gc --ignoreCache --noBuildLock --logLevel info",
    "watch": "hugo server --buildDrafts --buildFuture --buildExpired --gc --ignoreCache --noBuildLock --logLevel info --watch",
    "build": "hugo --gc --verbose --cleanDestinationDir --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Armoghans-Organization/Sleek-Stream.git"
  },
  "keywords": [
    "Hugo",
    "Custom Theme",
    "Custom",
    "Theme",
    "TailwindCss",
    "Scss",
    "Light Mode",
    "Dark Mode",
    "Light/Dark Mode",
    "Admin Panel",
    "Summery Panel",
    "Responsive",
    "Customizeable",
    "Template"
  ],
  "author": "Armoghan-ul-Mohmin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Armoghans-Organization/Sleek-Stream/issues"
  },
  "homepage": "https://github.com/Armoghans-Organization/Sleek-Stream#readme",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "boxicons": "^2.1.4",
    "npm-check": "^6.0.1",
    "rimraf": "^5.0.5",
    "vendor-copy": "^3.0.1"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.5"
  },
  "vendorCopy": [],
  "devVendorCopy": [
    {
      "from": "node_modules/@fortawesome/fontawesome-free/css/fontawesome.css",
      "to": "assets/vendors/fontawesome/fontawesome.css"
    },
    {
      "from": "node_modules/@fortawesome/fontawesome-free/js/fontawesome.js",
      "to": "assets/vendors/fontawesome/fontawesome.js"
    },
    {
      "from": "node_modules/boxicons/css/boxicons.css",
      "to": "assets/vendors/boxicons/boxicons.css"
    }
  ]
}